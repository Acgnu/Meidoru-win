<Page x:Class="AcgnuX.Pages.MobileSync.SyncPictures"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AcgnuX.Pages.MobileSync"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Loaded="OnPageLoaded"
      Title="SyncPictures">

    <Grid>
        <ListView Name="DeviceSyncListBox" Grid.Row="1" ItemContainerStyle="{StaticResource DeviceSyncListViewItemContainerStyle}" Template="{StaticResource DeviceSyncListViewTemplate}" Background="Transparent">
        </ListView>
    </Grid>
</Page>
